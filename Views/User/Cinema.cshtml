                                             @foreach (var item in Model)
                                            {
                                                <div class="cmsmasters_clients_item item">
                                                    <a href="https://mall.cmsmasters.net/fashion-brand/" target="_blank">
                                                        <img decoding="async" src="~/CinemasLogo/@Html.DisplayFor(modelItem => item.CinemaLogo)" alt="Shabby Chic" />@Html.DisplayFor(modelItem => item.CinemaName)<span>                                                          @Html.DisplayFor(modelItem => item.CinemaName)</span>
